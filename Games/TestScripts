-- Status
local status = Instance.new("TextLabel", G2L["2"])
status.Name = "SSTextLabel"
status.Text = "Status Script: nil"
status.Font = Enum.Font.Ubuntu
status.TextScaled = true
status.TextColor3 = Color3.fromRGB(255, 255, 255)
status.BackgroundTransparency = 1
status.Size = UDim2.new(0.57143, 0, 0.042, 0)
status.Position = UDim2.new(0.21333, 0, 0.53301, 0)
Instance.new("UIStroke", status).Thickness = 3

-- Buttons
local function createButton(text, pos, color, callback)
    local btn = Instance.new("TextButton", G2L["2"])
    btn.Text = text
    btn.Font = Enum.Font.Ubuntu
    btn.TextScaled = true
    btn.TextColor3 = color
    btn.BackgroundTransparency = 0.7
    btn.Size = UDim2.new(0.57143, 0, 0.1, 0)
    btn.Position = pos
    btn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0.05, 8)
    Instance.new("UIStroke", btn).Thickness = 3

    btn.MouseButton1Click:Connect(callback)
    return btn
end

-- Execute Script button
createButton("Execute Script!", UDim2.new(0.21429, 0, 0.682, 0), Color3.fromRGB(255, 255, 255), function()
    status.Text = "Status Script: Running..."
    task.wait(2)
    status.Text = "Status Script: Done!"
end)

-- Destroy GUI button
createButton("Destroy Gui", UDim2.new(0.21429, 0, 0.822, 0), Color3.fromRGB(255, 130, 130), function()
    G2L["1"]:Destroy()
end)

print("[✅] Global Script's GUI loaded successfully in CoreGui.")

