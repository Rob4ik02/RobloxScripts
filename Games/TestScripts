-- [[ TAB Misc ]] --

local MiscTab = Window:CreateTab({
    Name = "Misc",
    Icon = "extension",
    ImageSource = "Material",
    ShowTitle = true
})

local Label = MiscTab:CreateLabel({
	Text = "Player",
	Style = 2
})

MiscTab:CreateSlider({
	Name = "Speed",
	Range = {1, 500},
	Increment = 1,
	CurrentValue = 16,
	Callback = function(Value)
		player.Character.Humanoid.WalkSpeed = Value
	end
})

MiscTab:CreateSlider({
	Name = "Jump",
	Range = {1, 500},
	Increment = 1,
	CurrentValue = 50,
	Callback = function(Value)
		player.Character.Humanoid.JumpPower = Value
	end
})

local Label = MiscTab:CreateLabel({
	Text = "Camera",
	Style = 2
})

MiscTab:CreateSlider({
	Name = "Field of View",
	Range = {1, 120},
	Increment = 1,
	CurrentValue = 60,
	Callback = function(Value)
		workspace.CurrentCamera.FieldOfView = Value
	end
})

local Label = MiscTab:CreateLabel({
	Text = "Developers Tools (Use at your own risk)",
	Style = 2
})

local MiscButton = MiscTab:CreateButton({
	Name = "Btools",
	Description = "PC",
	Callback = function()
		Luna:Notification({
            Title = "Notification",
            Icon = "info",
            ImageSource = "Material",
            Content = "Executing Btools..."
        })
        loaded_notification:Play()
        loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/BTools.txt"))()
        if success then
            notification_success:Play()
            Luna:Notification({
                Title = "Success",
                Icon = "check_circle",
                ImageSource = "Material",
                Content = "btools executed successfully."
            })
            notification_success:Play()
        else
            Luna:Notification({
                Title = "Error",
                Icon = "error",
                ImageSource = "Material",
                Content = "Failed to execute btools"
            })
            notification_error:Play()
        end
	end
})

local MiscButton = MiscTab:CreateButton({
	Name = "F3X tools",
	Description = "PC",
	Callback = function()
		Luna:Notification({
            Title = "Notification",
            Icon = "info",
            ImageSource = "Material",
            Content = "Executing F3X tools..."
        })
        loaded_notification:Play()
        loadstring(game:GetObjects("rbxassetid://6695644299")[1].Source)()
