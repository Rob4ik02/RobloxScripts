local Players = game:GetService("Players")
local player = Players.LocalPlayer
local place = game.PlaceId
local regionserver = game:GetService("Workspace").CurrentCamera:FindFirstChild("RegionServer")
local PlaceId = game.PlaceId
local TweenService = game:GetService("TweenService");
local Debris = game:GetService("Debris");
local CoreGui = (gethui and gethui()) or game:GetService("CoreGui");

local ScreenGui = Instance.new("ScreenGui", CoreGui)
ScreenGui.Name = "Global Scripts"
ScreenGui.IgnoreGuiInset = true
ScreenGui.Enabled = false

local Background = Instance.new("Frame", ScreenGui)
Background.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Background.Size = UDim2.fromScale(1, 1)

local Warn = Instance.new("ImageLabel", Background)
Warn.Image = "rbxassetid://91154874732986" -- Replace with your image asset ID
Warn.Size = UDim2.new(0.3, 0, 0.5, 0)
Warn.AnchorPoint = Vector2.new(0.5, 0.5)
Warn.Position = UDim2.fromScale(0.5, 0.5)
Warn.BackgroundTransparency = 1
Warn.ImageTransparency = 1

wait(2)

ScreenGui.Enabled = true
wait(1)
for i = 1, 0, -0.01 do
	Warn.ImageTransparency = i
	wait(0.001)
end
wait(5)
for i = 0, 1, 0.01 do
	Warn.ImageTransparency = i
	wait(0.001)
end
wait(1)
ScreenGui.Enabled = false

wait(1) 
ScreenGui:Destroy()


local Luna = loadstring(game:HttpGet("https://raw.githubusercontent.com/Nebula-Softworks/Luna-Interface-Suite/refs/heads/master/source.lua", true))()

local Window = Luna:CreateWindow({
    Name = "Global Script's", -- Заголовок вашего окна
    Subtitle = "Hub Script's v1.1.0 | @universalscriptsroblox | Beta ", -- Подзаголовок
    LogoID = "91154874732986", -- ID логотипа
    LoadingEnabled = false, -- Включить анимацию загрузки
    LoadingTitle = "Loading script.", -- Заголовок для загрузки
    LoadingSubtitle = "Global Script's", -- Подзаголовок для загрузки

    ConfigSettings = {
        RootFolder = nil,
        ConfigFolder = " Global Script " -- Название папки для конфигураций
    },

    KeySystem = true, -- Включить систему ключей
    KeySettings = {
        Title = "Global Script's Key",
        Subtitle = "Key System",
        Note = "Please enter the key to use this script.",
        SaveInRoot = false,
        SaveKey = true,
        Key = {"GlobalScript_BetaKey2gg487sfhg542g41"},
        SecondAction = {
            Enabled = true,
            Type = "Telegram",
            Parameter = "t.me/@universalscriptsroblox"
        }
    }
})

