-- [[ TAB Misc ]] --

local MiscTab = Window:CreateTab({
    Name = "Misc",
    Icon = "extension",
    ImageSource = "Material",
    ShowTitle = true
})

local SliderSpeed = MiscTab:CreateSlider({
	Name = "Speed",
	Range = {1, 500},
	Increment = 1,
	CurrentValue = 16,
	Callback = function(Value)
		player.Character.Humanoid.WalkSpeed = Value
	end
}, "SliderSpeed")

local SliderJump = MiscTab:CreateSlider({
	Name = "Jump",
	Range = {1, 500},
	Increment = 1,
	CurrentValue = 50,
	Callback = function(Value)
		player.Character.Humanoid.JumpPower = Value
	end
}, "SliderJump")

local SliderFOV = MiscTab:CreateSlider({
	Name = "Field of View",
	Range = {1, 120},
	Increment = 1,
	CurrentValue = 60,
	Callback = function(Value)
		workspace.CurrentCamera.FieldOfView = Value
	end
}, "SliderFOV")

local Button1 = MiscTab:CreateButton({
	Name = "Infinite Yield",
	Description = nil,
	Callback = function()
        Luna:Notification({
                Title = "Notification",
                Icon = "info",
                ImageSource = "Material",
                Content = "Executing Infinite yield, please wait..."
            })
		loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
	end
})

local Dropdown1Dex = MiscTab:CreateDropdown({
	Name = "Dex Studio",
	Description = nil,
	Options = {
		"Dex v2", "Dex v3", "Dex v4"
	},
	CurrentOption = {"Dex v2"},
	MultipleOptions = false,
	SpecialType = nil,
	Callback = function(Options)
		if Options[1] == "Dex v2" then
            Luna:Notification({
                Title = "Notification",
                Icon = "info",
                ImageSource = "Material",
                Content = "Executing Dex v2, please wait..."
            })
            loadstring(game:HttpGet("https://raw.githubusercontent.com/MariyaFurmanova/Library/main/dex2.0", true))()
		elseif Options[2] == "Dex v3" then
            Luna:Notification({
                Title = "Notification",
                Icon = "info",
                ImageSource = "Material",
                Content = "Executing Dex v3, please wait..."
            })
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Babyhamsta/RBLX_Scripts/main/Universal/BypassedDarkDexV3.lua", true))()
		elseif Options[3] == "Dex v4" then
            Luna:Notification({
                Title = "Notification",
                Icon = "info",
                ImageSource = "Material",
                Content = "Executing Dex v4, please wait..."
            })
            loadstring(game:HttpGet("https://gist.githubusercontent.com/dannythehacker/1781582ab545302f2b34afc4ec53e811/raw/ee5324771f017073fc30e640323ac2a9b3bfc550/dark%2520dex%2520v4"))()
		end
	end
}, "Dropdown1Dex")


local Button1 = MiscTab:CreateButton({
	Name = "Destroy GUI",
	Description = nil,
	Callback = function()
        Luna:Notification({
            Title = "Warning",
            Content = "The gui will be destroyed after 5 seconds.",
            Duration = 5,
            Icon = "info",
	        ImageSource = "Material",
        })
        wait(5)
		Luna:Destroy()
	end
})

