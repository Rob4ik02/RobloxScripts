local Players = game:GetService("Players")
local player = Players.LocalPlayer
local place = game.PlaceId

local idPlaces = {
    Surviveinarea51 = 2214661900,
    BuildABoatForTreasure = 537413528
}

if place == idPlaces.Surviveinarea51 then

    print("Cooming soon...")

elseif place == idPlaces.BuildABoatForTreasure then

    local workspace = game:GetService("Workspace")
    local playersService = game:GetService("Players")
    local replicatedstorage = game:GetService("ReplicatedStorage")

    local player = playersService.LocalPlayer

    local Luna = loadstring(game:HttpGet("https://raw.githubusercontent.com/Nebula-Softworks/Luna-Interface-Suite/refs/heads/master/source.lua", true))()

    getgenv().SecureMode = _G.SecureMode
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Rob4ik02/RobloxScripts/refs/heads/main/Scripts/ESP.lua"))()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Rob4ik02/RobloxScripts/refs/heads/main/Scripts/Hitbox.lua"))()

    local Window = Luna:CreateWindow({
        Name = "Global Script's",
        Subtitle = "Build A Boat For Treasure",
        LogoID = 91154874732986,
        LoadingEnabled = true,
        LoadingTitle = "Oxygen 02",
        LoadingSubtitle = "Build A Boat For Treasure",
        ConfigSettings = {
            RootFolder = nil,
            ConfigFolder = "GlobalScripts",
        },
        KeySystem = false,
    })

    local FarmTab = Window:CreateTab({
        Name = "Auto Farm",
        Icon = "repeat",
        ImageSource = "Material",
        ShowTitle = true,
    })

    local Labelsoon1 = FarmTab:CreateLabel({
        Text = "Cooming soon...",
        Style = 1 -- Luna Labels Have 3 Styles : A Basic Label, A Green Information Label and A Red Warning Label. Look At The Following Image For More Details
    })

    local ShopTab = Window:CreateTab({
        Name = "Shop",
        Icon = "store",
        ImageSource = "Material",
        ShowTitle = true,
    })

    local Labelsoon1 = ShopTab:CreateLabel({
        Text = "Cooming soon...",
        Style = 1 -- Luna Labels Have 3 Styles : A Basic Label, A Green Information Label and A Red Warning Label. Look At The Following Image For More Details
    })

    local EspTab = Window:CreateTab({
        Name = "ESP",
        Icon = "visibility",
        ImageSource = "Material",
        ShowTitle = true -- This will determine whether the big header text in the tab will show
    })

    local Toggle1 = EspTab:CreateToggle({
    Name = "ESP",
    CurrentValue = false,
    Flag = "Toggle1",
        Callback = function(Value)
            if Value == false then
                _G.EspEnabled = false
                DeleteESP()
            else
                _G.EspEnabled = true
            end
        end,
    })

    local Toggle2 = EspTab:CreateToggle({
        Name = "ESP Visible Check",
        CurrentValue = false,
        Flag = "Toggle2",
            Callback = function(Value)
                _G.EspVisibleCheck = Value
                ESPDethMode()
            end,
    })

    local Toggle3 = EspTab:CreateToggle({
        Name = "Tracer Toggle",
        CurrentValue = false,
        Flag = "Toggle3",
            Callback = function(Value)
                _G.TracersVisible =Value
            end,
    })

    local Toggle4 = EspTab:CreateToggle({
        Name = "Tracer Team Check",
        CurrentValue = false,
        Flag = "Toggle4",
            Callback = function(Value)
                _G.TeamCheck =Value
            end,
    })

    local Slider1 = EspTab:CreateSlider({
	Name = "Trace Transparency",
	Range = {0, 1}, -- The Minimum And Maximum Values Respectively
	Increment = 0.1, -- Basically The Changing Value/Rounding Off
	CurrentValue = 0.7, -- The Starting Value
    	Callback = function(Value)
            _G.TracerTransparency=Value
    	end
    }, "Slider1") 

    local Slider2 = EspTab:CreateSlider({
	Name = "Trace Thickness",
	Range = {0, 100}, -- The Minimum And Maximum Values Respectively
	Increment = 1, -- Basically The Changing Value/Rounding Off
	CurrentValue = 1, -- The Starting Value
    	Callback = function(Value)
            _G.TracerThickness=Value
    	end
    }, "Slider2") 

    local PlayerTab = Window:CreateTab({
        Name = "Player",
        Icon = "person",
        ImageSource = "Material",
        ShowTitle = true -- This will determine whether the big header text in the tab will show
    })

    local SpeedSlider = PlayerTab:CreateSlider({
        Name = "Speed",
        Range = {0, 200}, -- The Minimum And Maximum Values Respectively
        Increment = 1, -- Basically The Changing Value/Rounding Off
        CurrentValue = 16, -- The Starting Value
            Callback = function(Value)
                game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
            end
    }, "Slider")

    local JumpSlider = PlayerTab:CreateSlider({
        Name = "Jump Power",
        Range = {0, 200}, -- The Minimum And Maximum Values Respectively
        Increment = 1, -- Basically The Changing Value/Rounding Off
        CurrentValue = 50, -- The Starting Value
        Callback = function(Value)
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
        end
    }, "Slider")

    local GravitySlider = PlayerTab:CreateSlider({
        Name = "Gravity",
        Range = {0, 200}, -- The Minimum And Maximum Values Respectively
        Increment = 1, -- Basically The Changing Value/Rounding Off
        CurrentValue = 196.2, -- The Starting Value
        Callback = function(Value)
            game.Workspace.Gravity = Value
        end
    }, "Slider")       

    local MiscTab = Window:CreateTab({
        Name = "Misc",
        Icon = "auto_awesome",
        ImageSource = "Material",
        ShowTitle = true -- This will determine whether the big header text in the tab will show
    })

    local Labelsoon1 = MiscTab:CreateLabel({
        Text = "Cooming soon...",
        Style = 1 -- Luna Labels Have 3 Styles : A Basic Label, A Green Information Label and A Red Warning Label. Look At The Following Image For More Details
    })

    local SettingsTab = Window:CreateTab({
        Name = "Settings",
        Icon = "settings",
        ImageSource = "Material",
        ShowTitle = true -- This will determine whether the big header text in the tab will show
    })

    local Toggle = SettingsTab:CreateToggle({
        Name = "Toggle Example",
        Description = nil,
        CurrentValue = false,
        Callback = function(Value)
            if Value == false then
                local replicatedstorage = game:GetService("ReplicatedStorage")
                local workspace = workspace or game:GetService("Workspace")
                local mainterrain = replicatedstorage:FindFirstChild("GlobalScripts_MainTerrain"):FindFirstChild("MainTerrain")
                if mainterrain then
                    mainterrain.Parent = workspace
                end
            else
                local workspace = workspace or game:GetService("Workspace")
                local mainterrain = workspace:FindFirstChild("MainTerrain")
                local replicatedstorage = game:GetService("ReplicatedStorage")
                if mainterrain then
                    replicatedstorage.Instance.new(Folder, "GlobalScripts_MainTerrain", {
                        Name = "MainTerrain",
                        Parent = mainterrain
                    })
                    mainterrain.Parent = replicatedstorage.GlobalScripts_MainTerrain
                end
            end
        end
    }, "Toggle")

    local ConfigTab = Window:CreateTab({
        Name = "Config Tab",
        Icon = "settings",
        ImageSource = "Material",
        ShowTitle = true
    })

    ConfigTab:BuildConfigSection()

    local SettingsSection = SettingsTab:CreateSection({
        Name = "Settings",
        Description = "Configure the script settings"
    })

    local CreditsTab = Window:CreateTab({
        Name = "Credits",
        Icon = "info",
        ImageSource = "Material",
        ShowTitle = true -- This will determine whether the big header text in the tab will show
    })

    local Label1 = CreditsTab:CreateLabel({
        Text = "Our Telegram: @universalscriptsroblox",
        Style = 2 -- Luna Labels Have 3 Styles : A Basic Label, A Green Information Label and A Red Warning Label. Look At The Following Image For More Details
    })

    local Label2 = CreditsTab:CreateLabel({
        Text = "Our Roblox: @8h8ph, @kiecsy, @RobTopBot_01.",
        Style = 1 -- Luna Labels Have 3 Styles : A Basic Label, A Green Information Label and A Red Warning Label. Look At The Following Image For More Details
    })

else
    local Luna = loadstring(game:HttpGet("https://raw.githubusercontent.com/Nebula-Softworks/Luna-Interface-Suite/refs/heads/master/source.lua", true))()

    local Window = Luna:CreateWindow({
        Name = "Global Script's",
        Subtitle = "Not supported game",
        LogoID = 91154874732986,
        LoadingEnabled = true,
        LoadingTitle = "Global Script's",
        LoadingSubtitle = "Not supported game",
        ConfigSettings = {
            RootFolder = nil,
            ConfigFolder = "GlobalScripts",
        },
        KeySystem = false,
    })


    Luna:Notification({
        Title = "Unsupported Game",
        Icon = "error",
        ImageSource = "Material",
        Content = "Did you know what? This script only works in Survive In Area 51 or Build A Boat For Treasure.\n\n" ..
                 "If you want to use it in other games, please contact the developer.",
    })

end
